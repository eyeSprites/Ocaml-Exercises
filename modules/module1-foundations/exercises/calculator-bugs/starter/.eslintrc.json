{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-undef": "error",
    "no-unreachable": "error",
    "no-fallthrough": "warn",
    "eqeqeq": "warn",
    "no-unused-vars": "warn",
    "no-constant-condition": "warn"
  }
}
